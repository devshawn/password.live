{
  "name": "server",
  "version": "0.2.0",
  "description": "Back-end server for password.live",
  "main": "server.js",
  "author": "Shawn Seymour <shawn@devshawn.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/",
    "serve-remote": "NODE_ENV=development nodemon server.js --exec babel-node",
    "serve": "nodemon server.js --exec babel-node",
    "build": "babel . --ignore 'node_modules,tests' -d dist",
    "dist": "npm run clean && npm run build",
    "test": "jest"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "password-generator": "^2.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest-cli": "^21.2.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "homepage": "https://github.com/devshawn/password.live",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devshawn/password.live.git"
  },
  "bugs": {
    "url": "https://github.com/devshawn/password.live/issues"
  }
}